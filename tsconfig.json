{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "strict": true,
    "noImplicitAny": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@capp/angular": [
        "projects/capp-angular/src/public-api"
      ],
      "@capp/angular-auth": [
        "projects/capp-angular-auth/src/public-api"
      ],
      "api": [
        "src/app/api-models"
      ],
      "shared/*": [
        "src/app/shared/*"
      ],
      "core/*": [
        "src/app/core/*"
      ],
      "common/*": [
        "src/app/common/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
